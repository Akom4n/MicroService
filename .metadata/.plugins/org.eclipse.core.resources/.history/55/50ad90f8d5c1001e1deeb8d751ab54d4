package com.akom4n.hrworker.entities;

public class Worker {

	private Long id;
	private String name;
	private Double dailyInCome;
	
	public Worker() {
	}

	public Worker(Long id, String name, Double dailyInCome) {
		super();
		this.id = id;
		this.name = name;
		this.dailyInCome = dailyInCome;
	}
}
